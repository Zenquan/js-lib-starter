{
  "name": "js-lib-starter",
  "version": "0.1.0",
  "description": "轮子模板",
  "main": "dist/index.js",
  "jsnext:main": "dist/index.esm.js",
  "module": "dist/index.esm.js",
  "type": "module",
  "sideEffects": false,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rimraf ./dist",
    "test": "npm run lint && cross-env NODE_ENV=test nyc mocha",
    "release": "npm test && npm run build && git commit -am $npm_package_version && git tag $npm_package_version && git push && git push --tags",
    "lint": "eslint -c .eslintrc.js 'src/**/*.js' && npm run lint:fix",
    "lint:fix": "eslint --fix -c .eslintrc.js 'src/**/*.js' --fix",
    "build:cjs": "rollup -c config/rollup.config.cjs.js",
    "build:cjs.min": "cross-env NODE_ENV=production npm run build:cjs",
    "build:esm": "rollup -c config/rollup.config.esm.js",
    "build:umd": "rollup -c config/rollup.config.umd.js",
    "build:umd.min": "cross-env NODE_ENV=production npm run build:umd",
    "build:lib": "rollup -c config/rollup.config.lib.js",
    "build": "npm run clean && npm run build:esm && npm run build:cjs && npm run build:cjs.min && npm run build:umd && npm run build:umd.min && npm run build:lib && npm run types",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "docs:jsdoc2md": "rimraf docs/pages/folder1/api.md && npx jsdoc2md src/* >> docs/pages/folder1/api.md",
    "docs:build": "npm run docs:jsdoc2md && npx vuepress build docs ",
    "docs:dev": "npm run docs:jsdoc2md && npx vuepress dev docs",
    "deploy": "npm run build && npm run docs:build",
    "deploy:gh": "gh-pages -d docs/.vuepress/dist",
    "commit:lint": "commitlint -e $HUSKY_GIT_PARAMS",
    "types": "rollup -c config/generate-dts.js"
  },
  "author": "zenquan",
  "license": "MIT",
  "husky": {
    "hooks": {
      "commit-msg": "#npm run commit:lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/zenquan/js-lib-starter.git"
  },
  "bugs": {
    "url": "https://github.com/zenquan/js-lib-starter/issues"
  },
  "files": [
    "/dist",
    "*.d.ts"
  ],
  "engines": {
    "node": ">= 0.12.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@rollup/plugin-commonjs": "^21.0.3",
    "@rollup/plugin-multi-entry": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.2.0",
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.165",
    "browserslist": "^4.20.2",
    "coveralls": "3.0.2",
    "cross-env": "5.2.0",
    "es5-shim": "4.5.10",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "esno": "^0.14.1",
    "expect.js": "0.3.1",
    "gh-pages": "^3.2.3",
    "husky": "3.1.0",
    "jsdoc": "^3.6.6",
    "jsdoc-to-markdown": "^6.0.1",
    "mocha": "3.5.3",
    "nyc": "13.1.0",
    "only-allow": "^1.1.0",
    "prettier": "^2.6.2",
    "rimraf": "2.6.2",
    "rollup": "^2.70.1",
    "rollup-plugin-dts": "^4.2.1",
    "typescript": "^4.6.3",
    "unplugin-swc": "^1.3.2",
    "vuepress": "^1.8.0"
  },
  "homepage": "https://github.com/zenquan/js-lib-starter#readme",
  "directories": {
    "doc": "docs",
    "test": "test"
  },
  "keywords": []
}
